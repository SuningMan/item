<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kuang.edu.dao.RegisterCodeDao">

    <resultMap type="com.kuang.edu.entity.RegisterCode" id="RegisterCodeMap">
        <result property="id" column="id" jdbcType="VARCHAR"/>
        <result property="orgId" column="org_id" jdbcType="VARCHAR"/>
        <result property="lastOrg" column="last_org" jdbcType="VARCHAR"/>
        <result property="ismanager" column="isManager" jdbcType="VARCHAR"/>
        <result property="registerNumber" column="register_number" jdbcType="VARCHAR"/>
        <result property="isuse" column="isUse" jdbcType="VARCHAR"/>
        <result property="createTime" column="create_time" jdbcType="VARCHAR"/>
        <result property="updateTime" column="update_time" jdbcType="VARCHAR"/>
    </resultMap>

    <!--查询单个-->
    <select id="queryById" resultMap="RegisterCodeMap">
        select
          id, org_id, last_org, isManager, register_number, isUse, create_time, update_time
        from wistron.register_code
        where id = #{id}
    </select>

    <!--查询指定行数据-->
    <select id="queryAllByLimit" resultMap="RegisterCodeMap">
        select
          id, org_id, last_org, isManager, register_number, isUse, create_time, update_time
        from wistron.register_code
        limit #{offset}, #{limit}
    </select>

    <!--通过实体作为筛选条件查询-->
    <select id="queryAll" resultMap="RegisterCodeMap">
        select
          id, org_id, last_org, isManager, register_number, isUse, create_time, update_time
        from wistron.register_code
        <where>
            <if test="id != null and id != ''">
                and id = #{id}
            </if>
            <if test="orgId != null and orgId != ''">
                and org_id = #{orgId}
            </if>
            <if test="lastOrg != null and lastOrg != ''">
                and last_org = #{lastOrg}
            </if>
            <if test="ismanager != null and ismanager != ''">
                and isManager = #{ismanager}
            </if>
            <if test="registerNumber != null and registerNumber != ''">
                and register_number = #{registerNumber}
            </if>
            <if test="isuse != null and isuse != ''">
                and isUse = #{isuse}
            </if>
            <if test="createTime != null and createTime != ''">
                and create_time = #{createTime}
            </if>
            <if test="updateTime != null and updateTime != ''">
                and update_time = #{updateTime}
            </if>
        </where>
    </select>

    <!--新增所有列-->
    <insert id="insert" keyProperty="id" useGeneratedKeys="true">
        insert into wistron.register_code(org_id, last_org, isManager, register_number, isUse, create_time, update_time)
        values (#{orgId}, #{lastOrg}, #{ismanager}, #{registerNumber}, #{isuse}, #{createTime}, #{updateTime})
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update wistron.register_code
        <set>
            <if test="orgId != null and orgId != ''">
                org_id = #{orgId},
            </if>
            <if test="lastOrg != null and lastOrg != ''">
                last_org = #{lastOrg},
            </if>
            <if test="ismanager != null and ismanager != ''">
                isManager = #{ismanager},
            </if>
            <if test="registerNumber != null and registerNumber != ''">
                register_number = #{registerNumber},
            </if>
            <if test="isuse != null and isuse != ''">
                isUse = #{isuse},
            </if>
            <if test="createTime != null and createTime != ''">
                create_time = #{createTime},
            </if>
            <if test="updateTime != null and updateTime != ''">
                update_time = #{updateTime},
            </if>
        </set>
        where id = #{id}
    </update>

    <!--通过主键删除-->
    <delete id="deleteById">
        delete from wistron.register_code where id = #{id}
    </delete>

</mapper>